<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <template id="assets" name="weighing_point assets">
            <link rel="stylesheet" type="text/scss" href="/web/static/src/scss/fonts.scss"/>
            <link rel="stylesheet" href="/web/static/lib/fontawesome/css/font-awesome.css"/>
            <link rel="stylesheet" href="/weighing_point/static/src/css/wp.css" id="wp-stylesheet"/>
            <link rel="stylesheet" href="/weighing_point/static/src/css/keyboard.css"/>

            <t t-if="request and request.env['res.lang'].search([('code', '=', request.env.lang)]).direction == 'rtl'">
                <link rel="stylesheet" type="text/less" href="/web/static/src/scss/fontawesome_overriden.scss"/>
            </t>

            <script type="text/javascript" src="/weighing_point/static/lib/waitfont.js"></script>
            <script type="text/javascript" src="/weighing_point/static/src/js/main.js"></script>
            <script type="text/javascript" src="/weighing_point/static/src/js/db.js"></script>
            <script type="text/javascript" src="/weighing_point/static/src/js/models.js"></script>
            <script type="text/javascript" src="/weighing_point/static/src/js/widget_base.js"></script>
            <script type="text/javascript" src="/weighing_point/static/src/js/keyboard.js"></script>
            <script type="text/javascript" src="/weighing_point/static/src/js/chrome.js"></script>
            <script type="text/javascript" src="/weighing_point/static/src/js/devices.js"></script>
            <script type="text/javascript" src="/weighing_point/static/src/js/gui.js"></script>
            <script type="text/javascript" src="/weighing_point/static/src/js/popups.js"></script>
            <script type="text/javascript" src="/weighing_point/static/src/js/screens.js"></script>
            <script type="text/javascript" src="/weighing_point/static/src/js/tests.js"></script>
        </template>

    <template id="FieldTextHtml">
        <t t-call="web_editor.layout">
            <div id="editable_area"
                 class="o_editable"
                 t-att-data-oe-model="model"
                 t-att-data-oe-field="field"
                 t-att-data-oe-id="res_id"
                 data-oe-type="html">
                <t t-raw="content" />
            </div>
        </t>
    </template>



</odoo>
